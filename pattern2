public class pattern2 {
    public static void main(String[] args) {
        
        for(int i= 0;i<=5;i++)
            {
                for(int j= 0;j<=5;j++)
                {   if(i==0||j==0||i==5||j==5)
                    {
                        System.out.print("*");

                    }
                    System.out.print("  ");
                   
                }
                System.out.println(" ");

        }   
    }
}
    
    
